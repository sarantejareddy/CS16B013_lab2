 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not (in = a, out = con1);
    Not (in = b, out = con2);
    Nand (a = con1, b = con2, out = out);
}
